<UserControl x:Class="QuizV2.ToolBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QuizV2"
             mc:Ignorable="d" 
             d:DesignHeight="25" d:DesignWidth="75">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="75">
            <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="25" Background="White" BorderBrush="Gray">
            </Button>
            <Button x:Name="button_Copy" Content="Button" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="25" Background="White" BorderBrush="Gray"/>
            <Button x:Name="button_Copy1" Content="Button" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="25" Background="White" BorderBrush="Gray"/>
        </StackPanel>

    </Grid>
</UserControl>
