<UserControl x:Class="QuizV2.TimerControl"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuizV2"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Height="160" Width="160">
    <Grid x:Name="Timer" Height="160" Width="160">
        <ProgressBar Name="snad" Style="{DynamicResource MaterialDesignCircularProgressBar}" Value="100" Width="150" Height="150" BorderThickness="10" IsEnabled="False"/>
        <ProgressBar Name="snad1" Style="{DynamicResource MaterialDesignCircularProgressBar}" Value="100" Width="140" Height="140" BorderThickness="10" IsEnabled="False"/>
        <TextBlock x:Name="TimerText" FontSize="45" VerticalAlignment="Center" HorizontalAlignment="Center" />
    </Grid>
</UserControl>